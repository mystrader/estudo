               </label>
              </div>
              <div class="checkbox-inline col-md-4">
               <input type="checkbox" id="pesquisar" name="pesquisar" value="Pesquisar">
               <label for="pesquisar">
                Pesquisar
               </label>
              </div>
           </div><!-- /Unidade -->
            
           <div class="input-group"><!-- Visualizar Log de Eventos -->
              <div class="checkbox-inline col-md-12">
              <input class="marcaTodos" type="checkbox" id="VisualizarLogdeEventos" name="VisualizarLogdeEventos" value="Visualizar Log de Eventos">
               <label class="h4" for="VisualizarLogdeEventos">
                Log de Eventos
               </label>
              </div><div class="clearfix"></div>
              <div class="checkbox-inline col-md-4">
              <input type="checkbox" id="consultar" name="consultar" value="Consultar">
               <label for="consultar">
                Consultar
               </label>
               </div>
              <div class="checkbox-inline col-md-4">
               <input type="checkbox" id="exportar" name="exportar" value="Exportar">
               <label for="exportar">
                Exportar
               </label>
              </div>
              <div class="checkbox-inline col-md-4">
               <input type="checkbox" id="imprimir" name="imprimir" value="Imprimir">
               <label for="imprimir">
                Imprimir
               </label>
              </div>
           </div><!-- /Visualizar Log de Eventos -->
           </div><!-- /col-md-6 -->
            
            <div class="col-md-6">
                           <div class="input-group"><!-- Visualizar Histórico -->
              <div class="checkbox-inline col-md-12">
              <input class="marcaTodos" type="checkbox" id="historico" name="historico" value="Historico">
               <label class="h4" for="historico">
               Históricos
               </label>
              </div><div class="clearfix"></div>
              <div class="checkbox-inline col-md-4">
              <input type="checkbox" id="consultar" name="consultar" value="Consultar">
               <label for="consultar">
                Consultar
               </label>
               </div>
              <div class="checkbox-inline col-md-4">
               <input type="checkbox" id="exportar" name="exportar" value="Exportar">
               <label for="exportar">
                Exportar
               </label>
              </div>
              <div class="checkbox-inline col-md-4">
               <input type="checkbox" id="imprimir" name="imprimir" value="Imprimir">
               <label for="imprimir">
                Imprimir
               </label>
              </div>
           </div><!-- / Histórico -->
           <div class="input-group"><!-- Visualizar Histórico -->
              <div class="checkbox-inline col-md-12">
              <input class="marcaTodos" type="checkbox" id="perfilfuncespecial" name="perfilfuncespecial" value="Perfil/Função Especial">
               <label class="h4" for="perfilfuncespecial">
               Perfil/Funcionalidade Especial
               </label>
              </div><div class="clearfix"></div>
              <div class="checkbox-inline col-md-4">
              <input type="checkbox" id="consultar" name="consultar" value="Consultar">
               <label for="consultar">
                Solicitar Perfil
               </label>
               </div>
              <div class="checkbox-inline col-md-4">
               <input type="checkbox" id="exportar" name="exportar" value="Exportar">
               <label for="exportar">
                Solicitar Funcionalidade
               </label>
              </div>
              <div class="checkbox-inline col-md-4">
               <input type="checkbox" id="fechar" name="fechar" value="fechar">
               <label for="fechar">
                Fechar
               </label>
              </div>
              <div class="checkbox-inline col-md-4">
               <input type="checkbox" id="incluirdocumento" name="incluirdocumento" value="incluirdocumento">
               <label for="incluirdocumento">
                Incluir Documento
               </label>
              </div>
              <div class="checkbox-inline col-md-4">
               <input type="checkbox" id="imprimir" name="imprimir" value="Imprimir">
               <label for="imprimir">
