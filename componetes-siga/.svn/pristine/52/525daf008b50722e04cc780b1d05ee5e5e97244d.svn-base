            <option selected disabled>Selecione</option>
            <option value="Comum">Fornecedor</option>
            <option value="Administrativo">Governo</option>
            <option value="Especial">Outros Entes</option>
           </select>
          </div><!-- /input-group -->

         </div><!-- /col-md-* -->
