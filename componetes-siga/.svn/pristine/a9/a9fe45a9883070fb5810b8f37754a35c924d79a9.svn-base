    <div class="panel panel-default">
     <header class="panel-heading" role="heading">
        <h4 class="panel-title">Buscar</h4>
    </header>
 
    <form class="form-group panel-body" action="#">
         
         <div class="row">
         
         <div class="col-md-12">
            <div class="input-group">
             <label for="licitacao">Licitação</label>
              <input class="form-control" name="licitacao" id="licitacao" placeholder="Número ou descrição da Licitação">
            </div><!-- /input-group -->
         </div>
         
         <div class="col-md-12">
             <hr>
             <h5>Filtros</h5>
         </div>

         <div class="col-md-12">
        <div class="input-group">
         <label for="modalidade">Modalidade</label>
          <select class="form-control" name="modalidade" id="modalidade">
              <option selected disabled>Selecione</option>
              <option value="concorrencia">Concorrência</option>
              <option value="preco">Tomada de Preço</option>
              <option value="convite">Convite</option>
          </select>
        </div><!-- /input-group -->
         </div>

         <div class="col-md-12">
        <div class="input-group">
         <label for="tipo">Tipo</label>
          <select class="form-control" name="tipo" id="tipo">
              <option selected disabled>Selecione</option>
              <option value="preco">Preço</option>
              <option value="tecnica">Técnica</option>
              <option value="tecnicaPreco">Técnica e Preço</option>
          </select>
        </div><!-- /input-group -->
         </div>

         <div class="col-md-12">
        <div class="input-group">
         <label for="forma">Forma</label>
          <select class="form-control" name="forma" id="forma">
              <option selected disabled>Selecione</option>
              <option value="0">Processo Normal</option>
              <option value="1">Maior Desconto Percentual</option>
              <option value="2">Menor Taxa Administrativa Percentual</option>
              <option value="3">Menor Valor de Taxa de Transação</option>
          </select>
        </div><!-- /input-group -->
         </div>

         <div class="col-md-12">
            <fieldset>
            <legend>Autorização de Lances ou Propostas</legend>
                <div class="alert alert-warning alert-sm" role="alert">
                    <i class="material-icons">&#xE001;</i> É necessário associar ao menos uma Funcionalidade
                </div>

                  <div class="input-group"><!-- .input-group -->
                    <div class="checkbox-inline col-lg-6">
                     <input type="radio" id="consultar" name="consultar" value="Consultar">
                     <label for="consultar">
                      Por Lote (Lances por valor global)
                     </label>
                    </div>

                    <div class="checkbox-inline col-lg-6">
                     <input type="radio" id="consultar" name="consultar" value="Consultar">
                     <label for="consultar">
                      Por Item
                     </label>
                    </div>
                  </div><!-- /.input-group -->
            </fieldset>
         </div>
         
         <div class="col-md-12">
          <div class="input-group">
            <div class="checkbox-inline col-md-12">
             <input type="checkbox" id="licitSustentavel" name="licitSustentavel" value="Consultar">
             <label for="licitSustentavel">
              Licitação Sustentável
             </label>
            </div>
          </div>

          <div class="input-group">
            <div class="checkbox-inline col-md-12">
             <input type="checkbox" id="homolParcial" name="homolParcial" value="Consultar">
             <label for="homolParcial">
              Licitação com Homologação Parcial
             </label>
            </div>
          </div>
         </div>
         
         <div class="col-md-12">
          <fieldset>
           <legend>Período</legend>
            <div class="input-group">
               <div class="col-lg-6">
                <label for="dataInicial">Data Inicial</label>
                <input id="dataInicial" name="dataInicial" class="form-control datepicker" type="text" placeholder="__/__/____"/>
               </div>
               <div class="col-lg-6">
                <label for="dataFinal">Data Final</label>
                <input id="dataFinal" name="dataFinal" class="form-control datepicker" type="text" placeholder="__/__/____"/>
               </div>
            </div>
          </fieldset>
                    <script>
                    </script>

         </div>
         

            <div class="col-md-12">
                <button type="button" class="btn btn-primary pull-right">Consultar <i class="material-icons first md">&#xE8B6;</i></button>
                <button type="button" class="btn btn-default" data-toggle="modal" data-target="#myModal">Limpar <i class="material-icons first md"><i class="material-icons">&#xE14C;</i></i></button>
            </div>
             
      </form>
